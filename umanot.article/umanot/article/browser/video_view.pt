<html  metal:use-macro="here/main_template/macros/master" i18n:domain="hero.contents">
<body>
  <div metal:fill-slot="content-core">
    <div class="addthis_sharing_toolbox"></div>
    <div id="video-view" tal:define="info view/info">
      <p  tal:condition="python:'iframe' in info['embed']" tal:replace="structure info/embed"></p>
      <iframe tal:condition="python:'iframe' not in info['embed']" width="853" height="480" tal:attributes="src info/embed" frameborder="0" allowfullscreen></iframe>
    </div> <!-- video-view -->
  </div>
</body>
</html>