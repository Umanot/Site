<html metal:use-macro="here/main_template/macros/master" i18n:domain="umanot.site">
  <head>
    
    
    
  </head>
  <body>
    
    <metal:headline fill-slot="headline_section_slot">

      <div id="service-folder-headline">
        <div class="gridOverlay"></div>
        <div class="headlineContent">
          <h1 class="documentFirstHeading" tal:content="view/title">Video</h1>
        </div>
      </div>

    </metal:headline>
    
    <metal:phil fill-slot="main">
      <tal:main-macro metal:define-macro="main">
      
      
        <div id="service-folder-view">
          <tal:groups repeat="group view/contents">
            <div class="mrow">
              <tal:loop repeat="item group">
                <div class="col-md-4" tal:condition="item">
                  <div class="serviceBlock">
                    <h3 tal:content="item/title">Coaching su Analisi Fisica<sup>®</sup> e su Umanot</h3>
                    <p tal:content="item/description">Corsi di formazione sui principi, le metodologie e l'operatività dell'Analisi Fisica e sulle caratteristiche dell'innovativo software di trading automatico Umanot.</p>
                    <a class="ghostButton ghostButtonWhite ghostButtonLg" tal:attributes="href item/URL" i18n:translate="Scopri come">Scopri come</a>
                  </div>
                </div>
              </tal:loop>
            </div>
          </tal:groups>
        </div>
        <!-- service-folder--view -->
      </tal:main-macro>
    </metal:phil>
  </body>
</html>